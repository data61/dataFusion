#! /not/to/be/execed
 
# these 3 used by sh/dfus
SCALA_VER=2.12
DFUS_DIR=/home/bac003/sw/dataFusion
DFUS_VER=0.2-SNAPSHOT

# sbt tests using MITIE will fail without these vars being appropriately set
export LD_LIBRARY_PATH=${DFUS_DIR}/dataFusion-ner/MITIE-native # directory containing libjavamitie.so
export NER_MITIE_ENGLISH_MODEL=${DFUS_DIR}/dataFusion-ner/MITIE-models/english/ner_model.dat
export NER_MITIE_SPANISH_MODEL=${DFUS_DIR}/dataFusion-ner/MITIE-models/spanish/ner_model.dat

export SEARCH_DOC_INDEX=${DFUS_DIR}/dataFusion-search/docIndex
export SEARCH_META_INDEX=${DFUS_DIR}/dataFusion-search/metaIndex
export SEARCH_NER_INDEX=${DFUS_DIR}/dataFusion-search/nerIndex

export PATH=${DFUS_DIR}/sh:/usr/sbin:/usr/bin:/sbin:/bin
